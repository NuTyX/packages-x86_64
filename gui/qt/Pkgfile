description="This port install all the qt parts"
url="http://qt-project.org/"
packager="pierre at nutyx dot org, tnut at nutyx dot org"

name=qt
version=5.8.0-rc
release=2
if [ ! -z ${_name} ]; then
  source=(https://download.qt.io/development_releases/qt/5.8/$version/submodules/${_name}-${version}.tar.xz)
fi
build() {
mkdir -p $PKG/usr
}
