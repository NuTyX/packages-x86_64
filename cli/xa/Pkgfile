#Depends on:  

description="Cross-assembler for 6502 microprocessor"
url="http://www.floodgap.com/retrotech/xa/"
packager="Greg"

name=xa
version=2.3.8

source=(http://www.floodgap.com/retrotech/xa/dists/xa-2.3.8.tar.gz)

build(){
	cd ${name}-${version}
	make
}

package() {
	cd ${name}-${version}
	make DESTDIR="${PKG}/usr" install
}
